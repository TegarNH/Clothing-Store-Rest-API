{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "import/no-dynamic-require": "off",
      "global-require": "off",
      "no-console": "off",
      "no-plusplus": "off",
      "no-restricted-globals": "off"
    }
}
